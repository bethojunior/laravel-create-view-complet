{
    "name": "bethojunior/makeview",
    "description": "Add command artisan for create complet view(css, js (modules, controllers) and view(blade) ) by Laravel",
    "require": {
        "php": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "bethojunior\\makeview\\": "src/"
        }
    },
    "scripts": {
        "init" : "@php src/init.php",
        "post-package-install": [
            "@php src/init.php"
        ],

        "post-autoload-dump": [
            "@php src/init.php"
        ]
    }
}
